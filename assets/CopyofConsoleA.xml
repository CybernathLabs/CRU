<?xml version="1.0" encoding="utf-8"?>
<controls>
	<control name="Hybercube Collator">
		<directives>
			<directive label="Set {name} to {state}" />
		</directives>
		<states>
			<state name="On">
				<input pin="7" value="1" />
				<output pin="19" value="1" />
			</state>
			<state name="Off" hidden="">
				<input pin="7" value="0" />
				<output pin="19" value="0" />
			</state>
		</states>
	</control>
	<control name="Solar Flywheel">
