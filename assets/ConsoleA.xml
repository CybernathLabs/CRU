<?xml version="1.0" encoding="utf-8"?>
<console>
	<controls>
		<control name="Hypercube Collator">
			<directives>
				<directive label="Set {name} to {state}" />
			</directives>
			<states>
				<state name="On">
					<input pin="7" value="1" />
					<output pin="59" value="1" />
				</state>
				<state name="Off" hidden="">
					<input pin="7" value="0" />
					<output pin="59" value="0" />
				</state>
			</states>
		</control>
		<control name="Solar Flywheel">
			<directives>
				<directive label="Set {name} to {state}" />
			</directives>
			<states>
				<state name="On" hidden="">
					<input pin="2" value="1" />
					<output pin="" value="" />
				</state>
				<state name="Off" hidden="">
					<input pin="2" value="0" />
					<output pin="" value="" />
				</state>
			</states>
		</control>
