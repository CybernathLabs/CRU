				</state>
				<state name="Off" hidden="">
					<input pin="3" value="0" />
					<output pin="54" value="0" />
				</state>
			</states>
		</control>
		<control name="Gravity Drive">
			<directives>
				<directive label="Set {name} to {state}" />
			</directives>
			<states>
				<state name="Disengaged" hidden="">
					<input pin="6" value="1" />
					<output pin="58" value="0" />
				</state>
				<state name="Engaged" hidden="">
					<input pin="6" value="0" />
					<output pin="58" value="1" />
				</state>
			</states>
		</control>
		<control name="Plutonium Grater">
			<directives>
				<directive label="Set {name} to {state}" />
			</directives>
			<states>
				<state name="On" hidden="">
					<input pin="9" value="1" />
					<output pin="" value="" />
				</state>
				<state name="Off" hidden="true">
					<input pin="9" value="0" />
					<output pin="" value="" />
				</state>
			</states>
		</control>
		<control name="Left Molex Coupling">
			<directives>
				<directive label="Set {name} to {state}" />
			</directives>
			<states>
