			<directives>
				<directive label="Set {name} to {state}" />
			</directives>
			<states>
				<state name="On" hidden="">
					<input pin="11" value="1" />
					<output pin="" value="" />
				</state>
				<state name="Off" hidden="">
					<input pin="11" value="0" />
					<output pin="" value="" />
				</state>
			</states>
		</control>
		<control name="Dyna-Cap">
			<directives>
				<directive label="Set {name} to {state}" />
			</directives>
			<states>
				<state name="Loosen" hidden="">
					<input pin="5" value="1" />
					<output pin="57" value="1" />
				</state>
				<state name="Off" hidden="true">
					<input pin="5" value="0" />
					<output pin="57" value="0" />
				</state>
			</states>
		</control>
		<control name="Dyna-cap">
			<directives>
				<directive label="Set {name} to {state}" />
			</directives>
			<states>
				<state name="Tighten" hidden="">
					<input pin="4" value="1" />
					<output pin="56" value="1" />
				</state>
				<state name="Off" hidden="">
					<input pin="4" value="0" />
					<output pin="56" value="0" />
				</state>
			</states>
		</control>
	</controls>
	<special>
		<output name="Light Flicker" minThreat="4" maxThreat="5" pin="12" activeValue="1" inactiveValue="0" />
		<output name="Eject Panel" minThreat="3" maxThreat="3" pin="53" activeValue="0" inactiveValue="1" />
		<timer pin="10" startValue="0" endValue="255" />
	</special>
</console>