		<control name="Krylon Shield">
			<directives>
				<directive label="Set {name} to {state}" />
			</directives>
			<states>
				<state name="On" hidden="">
					<input pin="8" value="1" />
					<output pin="13" value="1" />
				</state>
				<state name="Off" hidden="">
					<input pin="8" value="0" />
					<output pin="13" value="0" />
				</state>
			</states>
		</control>
		<control name="Orientation Laser">
			<directives>
				<directive label="Set {name} to {state}" />
			</directives>
			<states>
				<state name="On" hidden="">
					<input pin="3" value="1" />
